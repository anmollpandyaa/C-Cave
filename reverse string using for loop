#include<stdio.h>

int main()
{
    int size;

    printf("Enter size of the string: ");
    scanf("%d", &size);

    char arr[size];

    for (int i = 0; i < size; i++)
    {
        printf("Enter element %d: \n", i+1);
        scanf(" %c", &arr[i]);
    }

    for (int i = size - 1; i >= 0; i--)
    {
        printf(" %c", arr[i]);
    }

    return 0;  
}
