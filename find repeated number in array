#include<stdio.h>

int main()
{
    int arr[] = {2, 4, 16, 4, 7, 9, 2};

    for (int i = 0; i < 7; i++)
    {
        for (int j = i+1; j < 7; j++)
        {
            if (arr[i] == arr[j])
            {
                printf("%d\t", arr[i]);
                break;
            }
        }   
    }
    return 0;
}
